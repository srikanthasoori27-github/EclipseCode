<Custom name="Custom-Framework-Exclusion-Logical-BusinessApplications-PrivilegedAccess">
	<Attributes>
		<Map>
		<!-- Mover and Manager certification -->
			<entry key="LogicalBusinessApps">
				<value>
					<List>
                   </List>
				</value>
			</entry>		
			<!-- Mover Certification -->
			<entry key="excludePrivilegedAccess" value="FALSE"/>
			<!-- Manager Certification -->
			<entry key="excludePrivilegedAccessRegularCert" value="FALSE"/>
			<!-- Application Owner Certification -->
			<entry key="includeOnlyPrivilegedAccessApplicationOwnerCert" value="FALSE"/>
			<!-- Entitlement Owner Certification -->
			<entry key="includeOnlyPrivilegedAccessEntitlementOwnerCert" value="FALSE"/>
			<!-- Account Group Membership Certification -->
			<entry key="includeOnlyPrivilegedAccessAccountGroupMemCert" value="FALSE"/>
			<!-- Native Change Certification -->
			<entry key="includeOnlyPrivilegedAccessNativeChangeCert" value="FALSE"/>
		</Map>
	</Attributes>
</Custom>
