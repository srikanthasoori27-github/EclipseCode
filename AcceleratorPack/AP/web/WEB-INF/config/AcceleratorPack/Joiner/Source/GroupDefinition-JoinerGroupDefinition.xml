<GroupDefinition indexed="true" name="New Joiner Has At least One Birthright Account" private="true">
<!-- Any identity that has atleast one joiner application-->
  <GroupFilter>
    <CompositeFilter operation="AND">
      <CompositeFilter operation="AND">
        <Filter operation="EQ" property="links.application.joinerEnabled" ignoreCase="true" value="TRUE">
        </Filter>
       <Filter operation="EQ" property="correlated">
        <Value>
          <Boolean>true</Boolean>
        </Value>
      </Filter>
      </CompositeFilter>
    </CompositeFilter>
  </GroupFilter>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
</GroupDefinition>
