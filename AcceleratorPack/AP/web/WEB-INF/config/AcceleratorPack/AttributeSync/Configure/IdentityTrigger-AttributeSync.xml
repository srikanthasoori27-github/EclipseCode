<IdentityTrigger disabled="false" handler="sailpoint.api.WorkflowTriggerHandler" name="ATTRIBUTE SYNCHRONIZATION FEATURE" type="Rule">
  <Description>An Identity attribute was changed that was sycnhronized with downstream applications.</Description>
  <HandlerParameters>
    <Attributes>
      <Map>
        <entry key="workflow" value="Workflow-Lifecycle-AttributeSync-RequestObject-Wrapper"/>
      </Map>
    </Attributes>
  </HandlerParameters>
  <TriggerRule>
    <Reference class="sailpoint.object.Rule" name="Rule-IdentityTrigger-AttributeSync"/>
  </TriggerRule>
</IdentityTrigger>
