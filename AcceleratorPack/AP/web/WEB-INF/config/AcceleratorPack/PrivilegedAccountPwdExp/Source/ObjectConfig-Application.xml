<sailpoint>
<ImportAction name='merge'>
<ObjectConfig name="Application">
	<!-- PRIVILEGED ACCOUNT PASSWORD EXP START -->
	<ObjectAttribute categoryName="Privileged Account"
		defaultValue="" displayName="Enable Password Expiration Notification" namedColumn="true" editMode="Permanent" name="enablepswexpNotification" type="string">
		<Description>Enable Password Expiration Notification</Description>
	</ObjectAttribute>
		<ObjectAttribute categoryName="Privileged Account"
		defaultValue="" displayName="Password Reset After X Days" editMode="Permanent" name="xdaysresetPwd" type="string">
		<Description>Password Reset After X days</Description>
	</ObjectAttribute>
	<ObjectAttribute categoryName="Privileged Account"
		defaultValue=""  displayName="Password Expiration Application Attribute" editMode="Permanent" name="pwdexpAttr" type="string">
		<Description>Password Expiration Attribute</Description>
	</ObjectAttribute>
	<ObjectAttribute categoryName="Privileged Account"
		defaultValue="" displayName="Password Before Expiration Days (Daily Notfications)" editMode="Permanent" name="beforepwdexpAttr" type="string">
		<Description>Password Before Expiration Day (Daily Notfications)</Description>
	</ObjectAttribute>
		<ObjectAttribute categoryName="Privileged Account"
		defaultValue="" displayName="Password After Expiration Days (Action on Account)" editMode="Permanent" name="afterpwdexpAttr" type="string">
		<Description>Password After Expiration Day (Action on Account)</Description>
	</ObjectAttribute>
	<ObjectAttribute categoryName="Privileged Account" defaultValue="" displayName="Password Expiration Date Format Options" editMode="Permanent" name="pwdexpAttrexpression">
		<Description>Password Format to be converted to Date</Description>
		<AllowedValues>
			<String>Windows NT Time Format</String>
			<String>YYYYMMDDHHMMSST Format</String>
			<String>Extended Rule</String>
		</AllowedValues>
	 </ObjectAttribute>
		<ObjectAttribute categoryName="Privileged Account"
		defaultValue="" displayName="Password Expiration Date Rule" editMode="Permanent" name="pwdexpAttrexpressionRule" type="string">
		<Description>Return Password Expiration Date</Description>
	</ObjectAttribute>
	<ObjectAttribute categoryName="Privileged Account"
		defaultValue="" displayName="Scramble Password Application Attribute" editMode="Permanent" name="scramPwdattr" type="string">
		<Description>Password Attribute to be scrambled, not required for connectors that supports password operation</Description>
	</ObjectAttribute>
	<ObjectAttribute categoryName="Privileged Account"
		defaultValue="" displayName="After Password Expiration Options"
		editMode="Permanent" namedColumn="true" name="afterPassExpProcess">
		<AllowedValues>
			<String>Do Nothing</String>
			<String>Scramble Password</String>
			<String>Disable Account and Scramble Password</String>
		</AllowedValues>
		<Description>After Password Expiration Options</Description>
	</ObjectAttribute>
	<!-- PRIVILEGED ACCOUNT PASSWORD EXP END -->
	</ObjectConfig>
	</ImportAction>
</sailpoint>
