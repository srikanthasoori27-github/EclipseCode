<Custom name="Custom-Mover-Settings">
  <Attributes>
    <Map>
      <entry key="MOVER SETTINGS">
       <value>
         <Map>
            <entry key="certificationSettingsMappings">
              <value>
                <Map>
                  <entry key="MOVER FEATURE">
                    <value>
                      <List>
                        <Map>
                          <!--Certification Name Start-->
                          <entry key="certificationNamePrefix" value="Mover Event For "/>
                          <entry key="certificationNameSuffix" value=" Assigned To"/>
                          <!--Certification Name End-->
                          <!-- Staging Start -->
                          <entry key="stagingEnabled" value="true"/>
                           <!-- Staging End -->
                         </Map>
                      </List>
                    </value>
                  </entry>
                 </Map>
               </value>
            </entry>
          </Map>
        </value>
      </entry>
    </Map>
  </Attributes>
</Custom>
