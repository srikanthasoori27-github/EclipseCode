<sailpoint>
<ImportAction name='merge'>
<ObjectConfig name="Application">
	<!-- JOINER REVERSE LEAVER -->
	<ObjectAttribute categoryName="Joiner" defaultValue="" displayName="Reverse Leaver Options"
		editMode="Permanent" namedColumn="true" name="reverseleaverProcess" required="true">
		<AllowedValues>
			<String>Do Nothing</String>
			<String>Enable Immediately</String>
			<String>Enable Immediately and Move Account</String>
			<String>Enable Immediately, Restore Birthright Access, and Move Account</String>
			<String>Restore All Access Immediately</String>
			<String>Unlock Only</String>
			<String>Unlock Only and Move Account</String>
			<String>Unlock Only, Restore Birthright Access, and Move Account</String>
			<String>Extended Rule</String>
		</AllowedValues>
	<Description>Reverse of Leaver, Pending X Days Requests will be removed </Description>
	</ObjectAttribute>
	<ObjectAttribute defaultValue="" categoryName="Joiner"
		displayName="Reverse Termination Extended Rule" editMode="Permanent" name="reverseTerminationExtendedRule">
		<Description>This rule will be executed in case selected Rehire Options is Extended Rule</Description>
	</ObjectAttribute>
	<ObjectAttribute defaultValue="" categoryName="Joiner"
		displayName="Reverse Leaver Options Per Population" editMode="Permanent"
		name="apPopulationReverseLeaverOptions">
		<Description>List of Map Options. Please see AP Joiner documentation</Description>
	</ObjectAttribute>
	</ObjectConfig>
	</ImportAction>
</sailpoint>
