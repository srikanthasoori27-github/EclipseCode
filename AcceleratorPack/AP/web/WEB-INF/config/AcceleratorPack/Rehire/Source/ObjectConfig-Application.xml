<sailpoint>
<ImportAction name='merge'>
<ObjectConfig name="Application">
<!-- JOINER REHIRE -->
		<ObjectAttribute categoryName="Joiner" defaultValue="" displayName="Rehire Options"
		editMode="Permanent" namedColumn="true" name="rehireProcess" required="true">
		<AllowedValues>
			<String>Do Nothing</String>
			<String>Enable Immediately</String>
			<String>Enable Immediately and Reset Password</String>
			<String>Enable Immediately and Restore Account</String>
			<String>Enable Immediately and Restore Account and Reset Password</String>
			<String>Unlock Only</String>
			<String>Unlock Only and Reset Password</String>
			<String>Unlock Only and Restore Account</String>
			<String>Unlock Only and Restore Account and Reset Password</String>
			<String>Extended Rule</String>
		</AllowedValues>
	 <Description>Employee Type Change (Contractor To Employee or Employee To Contractor) or Same Employee Type</Description>
	</ObjectAttribute>
		<ObjectAttribute categoryName="Joiner" defaultValue="" displayName="Process Attribute Synchronization After Rehire" editMode="Permanent" name="apAfterRehireAttrSync" type="boolean" >
	<Description>This can be used when there are common Identity Trigger Attributes for Attribute Sync and Rehire</Description>
	</ObjectAttribute>
	<ObjectAttribute defaultValue="" categoryName="Joiner" 
		displayName="Rehire Extended Rule" editMode="Permanent" name="rehireExtendedRule">
		<Description>This rule will be executed in case selected Rehire Options is Extended Rule</Description>
	</ObjectAttribute>
	</ObjectConfig>
	</ImportAction>
</sailpoint>
