<sailpoint>
<ImportAction name='merge'>
<ObjectConfig name="Application">
<!-- LEAVER LTD -->
	<ObjectAttribute categoryName="Leaver" defaultValue="" displayName="Long Term Disability Options" editMode="Permanent"
		namedColumn="true" name="ltdProcess" required="true">
		<AllowedValues>
			<String>Do Nothing</String>
			<String>Disable Only</String>
			<String>Disable and Remove Entitlements</String>
			<String>Remove Entitlements</String>
			<String>Delete Immediately</String>
			<String>Lock Only</String>
			<String>Extended Rule</String>
		</AllowedValues>
	 <Description>Long Term Disability Options</Description>
	</ObjectAttribute>
	<ObjectAttribute categoryName="Leaver"
		defaultValue="" displayName="Long Term Disability Extended Rule" editMode="Permanent" name="ltdExtendedRule">
		<Description>This rule will be executed in case Long Term Disability Options is Extended Rule</Description>
	</ObjectAttribute>
	</ObjectConfig>
	</ImportAction>
</sailpoint>
