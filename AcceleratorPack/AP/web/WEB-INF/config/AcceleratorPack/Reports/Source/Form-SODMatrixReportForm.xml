<Form hidden="true" name="Form-SODMatrixReportForm" type="Report">
	<Section columns="1" label="Report Properties" name="customProperties">
		<Field displayName="List of applications" name="applications" helpKey="Manually select the applications to include in the report" type="String" multi="true" value="ref:applications">
			<AllowedValuesDefinition>
				<Script>
					<Source>
						<![CDATA[
						import sailpoint.rapidapponboarding.rule.PolicyViolationsRuleLibrary;
						List appList= new ArrayList();
						List pvAppList = PolicyViolationsRuleLibrary.getPVAppList(context);
						if(pvAppList!=null && pvAppList.size()>0)
						{
						appList.addAll(pvAppList);
						return appList;
						}
						]]>
					</Source>
				</Script>
			</AllowedValuesDefinition>
		</Field>
		<Field name="selectAllApplications" displayName="Use all applications" helpKey="Automatically include all  applications" type="boolean" value="ref:selectAllApplications"/>
	</Section>
</Form>
