      <AllowedValues>
        <List>
          <String>null</String>
          <String>rept_role_members_filter_status_all</String>
        </List>
        <List>
          <String>true</String>
          <String>rept_role_members_filter_status_disabled</String>
        </List>
        <List>
          <String>false</String>
          <String>rept_role_members_filter_status_enabled</String>
