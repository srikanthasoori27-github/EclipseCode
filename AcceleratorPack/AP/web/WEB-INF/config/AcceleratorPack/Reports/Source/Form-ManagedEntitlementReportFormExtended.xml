    <Field displayName="Entitlement End Date" helpKey="Entitlement End Date" name="creationDateEnd" type="date" value="ref:creationDateEnd">
      <Attributes>
        <Map>
          <entry key="endDate" value="true"/>
        </Map>
      </Attributes>
    </Field>
  </Section>
</Form>
