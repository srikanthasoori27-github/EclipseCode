<Form hidden="true" name="Managed Entitlement Report Form Extended" type="Report">
  <Section columns="1" label="Managed Entitlement Arguments" name="customProperties">
    <Field displayName="Requestable" helpKey="Requestable" name="requestable" type="string" value="ref:requestable">
      <AllowedValues>
        <List>
          <String>null</String>
          <String>All</String>
        </List>
        <List>
          <String>true</String>
          <String>Requestable Only</String>
        </List>
        <List>
          <String>false</String>
          <String>Non Requestable</String>
        </List>
      </AllowedValues>
    </Field> 
    <Field displayName="applications" helpKey="help_rept_identity_entitlement_applications" multi="true" name="application" type="Application" value="ref:application"/>
    <!-- Addition of Business App Name and Privileged Flag Start-->
    <!-- Addition of Business App Name and Privileged Flag End-->
