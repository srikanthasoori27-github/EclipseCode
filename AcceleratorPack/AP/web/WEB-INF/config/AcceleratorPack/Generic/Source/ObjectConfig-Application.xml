<sailpoint>
<ImportAction name='merge'>
<ObjectConfig name="Application">
	<!-- USED BY AGGREGATION, CERTIFICATION, LEAVER, APPROVAL, REPORTING -->
	<ObjectAttribute categoryName="Aggregate"
		displayName="Is Business Application" namedColumn="true" editMode="Permanent" name="busApp" type="boolean" defaultValue="">
		<Description>Used By Certification, Leaver, and Approval</Description>
	</ObjectAttribute>
	<ObjectAttribute defaultValue="" categoryName="Aggregate"
		displayName="Additional Global Customization Rule" editMode="Permanent"
		name="apAdditionalApplicationCustomizationRule">
		<Description>Additional Accelerator Pack Global Customization Rule</Description>
	</ObjectAttribute>
	<ObjectAttribute defaultValue="" categoryName="Aggregate"
		displayName="Additional Creation Rule" editMode="Permanent"
		name="apAdditionalApplicationCreationRule">
		<Description>Additional Accelerator Pack Creation Rule</Description>
	</ObjectAttribute>
	<ObjectAttribute defaultValue="" categoryName="Aggregate"
		displayName="Additional Correlation Rule" editMode="Permanent"
		name="apAdditionalApplicationCorrelationRule">
		<Description>Additional Accelerator Pack Correlation Rule</Description>
	</ObjectAttribute>
	<ObjectAttribute defaultValue="" categoryName="Aggregate"
		displayName="Additional Managed Attribute Customization Rule" editMode="Permanent"
		name="apAdditionalApplicationMCustomizationRule">
		<Description>Additional Accelerator Pack Managed Attribute Customization Rule</Description>
	</ObjectAttribute>
	<ObjectAttribute defaultValue="" categoryName="Aggregate"
		displayName="Additional Managed Attribute Group Refresh Rule" editMode="Permanent"
		name="apAdditionalApplicationMGroupRefreshRule">
		<Description>Additional Accelerator Pack Managed Attribute Group Refresh Rule</Description>
	</ObjectAttribute>
	<ObjectAttribute defaultValue="" categoryName="Aggregate"
		displayName="Additional After Provisioning Rule" editMode="Permanent"
		name="apAdditionalApplicationAfterProvisioningRule">
		<Description>Additional Accelerator Pack After Provisioning Rule</Description>
	</ObjectAttribute>
	<ObjectAttribute defaultValue="" categoryName="Aggregate"
		displayName="Additional Pre Iterate Rule" editMode="Permanent"
		name="apAdditionalApplicationPreIterateRule">
		<Description>Additional Accelerator Pack Pre Iterate Rule</Description>
	</ObjectAttribute>
    <ObjectAttribute defaultValue="" categoryName="Policy Violation"
            displayName="Allow Access Request Policy Names" editMode="Permanent"
            name="softPolicyviolations"  namedColumn="true" type="string">
            <Description>Comma Separated Policy Names</Description>
    </ObjectAttribute>
    <ObjectAttribute defaultValue="" categoryName="Policy Violation"
            displayName="Deny Access Request Policy Names" editMode="Permanent"
            name="hardPolicyviolations" namedColumn="true" type="string">
            <Description>Comma Separated Policy Names</Description>
    </ObjectAttribute>
	</ObjectConfig>
	</ImportAction>
</sailpoint>
