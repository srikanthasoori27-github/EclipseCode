<GroupDefinition indexed="true" name="Accelerator Pack Correlated Identities" private="true">
  <GroupFilter>
    <CompositeFilter operation="AND">
      <Filter operation="EQ" property="correlated">
        <Value>
          <Boolean>true</Boolean>
        </Value>
      </Filter>
    </CompositeFilter>
  </GroupFilter>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
</GroupDefinition>
