<Custom name="Custom-Triggers">
  <Attributes>
    <Map>
      <entry key="TRIGGERS">
        <value>
          <Map>
            <entry key="Authoritative Sources Triggers">
              <value>
                <Map>
                  <entry key="joinerProcess">
                    <value>
                      <List>
                        <Map>
                          <entry key="Operation" value="AND"/>
                        </Map>
                        <Map>
                        <!-- Implicit Joiner Event, Any Cube that doesn't have any Joiner Application is eligible for Joiner
                        This takes care of Birthright Roles not being assigned to cubes that are pre-processed before IdentityIQ takes over-->
                          <entry key="Attribute" value="GROUPDEFINITION"/>
                          <entry key="NewValues" value="New Joiner Has At least One Birthright Account"/>
                          <entry key="OldValues" value="IGNORE"/>
                        </Map>
                        <!-- Implicit Joiner Event, All Authoritative Source Cubes on Creation would have "NEEDS PROCESSING" value for attribute "needsJoiner" 
                        This is done via rule "Rule-FrameWork-Creation-Joiner-Authoritative-Source"-->
                        <Map>
                          <entry key="Attribute" value="needsJoiner"/>
                          <entry key="NewValues" value="NEEDS PROCESSING"/>
                          <entry key="OldValues" value="IGNORE"/>
                        </Map>
                      </List>
                    </value>
                  </entry>
                  <entry key="loaProcess">
                  </entry>
                  <entry key="ltdProcess">
                  </entry>
                  <entry key="moverProcess">
                  </entry>
                  <entry key="rehireProcess">
                  </entry>
                  <entry key="reverseleaverProcess">
                  </entry>
                  <entry key="rtwloaProcess">
                  </entry>
                  <entry key="rtwltdProcess">
                  </entry>
                  <entry key="terminationProcess">
                  </entry>
                </Map>
              </value>
            </entry>
          </Map>
        </value>
      </entry>
    </Map>
  </Attributes>
</Custom>
