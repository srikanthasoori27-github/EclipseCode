<sailpoint>
	<ImportAction name="merge">
		<Configuration name="SystemConfiguration">
			<Attributes>
				<Map>
					 <entry key="quickLinkCategories">
				        <value>
				          <List>
				           <QuickLinkCategory cssClass="quickLinkCategoryCustom" messageKey="quicklink_category_smart_config" name="SmartConfig" ordering="5"/>
				          </List>
				        </value>
      				</entry>
      				 <!--Archive Work Items -->
					 <entry key="workItemArchiveTypes" value="Certification, Remediation, Delegation, Approval, ViolationReview, Form, PolicyViolation, Challenge, ImpactAnalysis, Signoff, ManualAction"/>
     				<!--Performance Improvement Updates -->
					<entry key="allowWorkgroupsInIdListBean" value="false" />
					<entry key="defaultHomeWidgets" value="Approvals, Violations, DirectReports, MyAccessReviews"/>
					 <!-- Quick Link Cards -->
                    <entry key="defaultQuickLinkCards" value="Policy Violations, Access Reviews, Approvals, Request Access, Track Requests, Manage Accounts, Work Items, Mobile Violation Reviews, Forms, View Identity"/>
           	</Map>
			</Attributes>
		</Configuration>
	</ImportAction>
</sailpoint>
