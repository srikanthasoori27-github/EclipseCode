<Custom name="Custom-Ticket-Settings">
  <Attributes>
    <Map>
      <entry key="TICKET SETTINGS">
        <value>
          <Map>
            <!--  The maximum number of status checks that will be tried before failure 
       		of the provisioning -->
             <entry key="provisioningMaxStatusChecks">
             <value>
             <Integer>3</Integer>
             </value>
             </entry> 
             <!--   The interval in minutes to retry the check status call -->
              <entry key="provisioningStatusCheckInterval">
             <value>
             <Integer>720</Integer>
             </value>
             </entry> 
              <entry key="optimisticProvisioningTicket" value="True"/>
             </Map>
        </value>
      </entry>
    </Map>
  </Attributes>
</Custom>
