<Custom name="Custom-ApprovalFrameWorkMappings">
	<Attributes>
		<Map>
			<entry key="Eligible Request Type Approvals">
				<value>
					<Map>
						<!-- Interactive -->
						<entry key="CREATE IDENTITY FEATURE"><value><List><String>ManagerOrServiceAccountOwner</String><String>Workgroup</String></List></value></entry>
						<!-- Interactive -->
						<entry key="EDIT IDENTITY FEATURE"><value><List><String>ManagerOrServiceAccountOwner</String><String>Workgroup</String></List></value></entry>
						<!-- Interactive -->
						<entry key="REGISTRATION FEATURE"><value><List><String>ManagerOrServiceAccountOwner</String><String>Workgroup</String></List></value></entry>
						<!-- SCIM/REST -->
						<entry key="EXTERNAL FEATURE"></entry>
						<!-- Interactive -->
						<entry key="RECOVERY TOOL FEATURE"></entry>
						<!-- Interactive -->
						<entry key="REQUEST MANAGER FEATURE"></entry>
						<!-- Interactive -->
						<entry key="CHANGE PASSWORD FEATURE"></entry>
						<!-- Interactive -->
						<entry key="MANAGE PASSWORDS FEATURE"></entry>
						<!-- Interactive -->
						<entry key="PASSWORD INTERCEPTOR FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Interactive -->
						<entry key="IMMEDIATE TERMINATION FEATURE"></entry>
						<!-- Interactive -->
						<entry key="FIREFIGHTER FEATURE"></entry>
						<!-- Interactive -->
						<entry key="SUSPEND FEATURE"></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="JOINER LTD FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="JOINER LOA FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="REVERSE LEAVER FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="JOINER FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="JOINER REHIRE FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING, Not Really Used, Certifications can be staged using Smart Services common settings -->
						<entry key="MOVER FEATURE"></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="MANAGER CERTIFICATION ALLOW EXCEPTION FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="LEAVER FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING -->
					    <entry key="LEAVER LTD FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="LEAVER LOA FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- AFTER PSA PASSWORD EXPIRATION ACTION STAGING -->
						<entry key="PRIVILEGED SERVICE PASSWORD EXPIRATION FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="ATTRIBUTE SYNCHRONIZATION FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="NATIVE CHANGE DETECTION FEATURE"><value><List><String>Operations</String></List></value></entry>
						<!-- Life Cycle EVENT STAGING -->
						<entry key="SER LINK FEATURE"></entry>
						<!-- Interactive -->
						<entry key="SER UNLINK FEATURE"></entry>
						<!--  5 Maximum Approvals -->
						<entry key="CART REQUEST FEATURE">
						<value>
						<List>
						<String>ManagerOrServiceAccountOwner</String>
						<String>ApplicationBusApprovers</String>
						<String>AdditionalApplicationBusApprovers</String>
						<String>EntitlementBusApprovers</String>
						<String>AdditionalEntitlementBusApprovers</String>
						<String>RoleBusApprovers</String>
						<String>AdditionalRoleBusApprovers</String>
						</List>
						</value>
						</entry>
						<!--   3 Max Approvals -->
						<entry key="MANAGE ACCOUNTS FEATURE">
						<value>
						<List>
						<String>ManagerOrServiceAccountOwner</String>
						<String>ApplicationBusApprovers</String>
						<String>AdditionalApplicationBusApprovers</String>
						</List>
						</value>
						</entry>
						<!--   1 Batch Approval + Access Request Approval / Manage Account Approvals-->
						<entry key="BATCH REQUEST FEATURE"><value><List><String>BatchOwners</String></List></value></entry>
					</Map>
				</value>
			</entry>
			<entry key="Split Eligibility">
				<value>
					<Map>
						<entry key="RoleBusApprovers" value="false"/>
						<entry key="AdditionalRoleBusApprovers" value="false"/>
						<entry key="AdditionalEntitlementBusApprovers" value="false"/>
						<entry key="AdditionalApplicationBusApprovers" value="false"/>
						<entry key="ApplicationBusApprovers" value="false"/>
						<entry key="EntitlementBusApprovers" value="false"/>
						<entry key="BatchOwners" value="false"/>
						<entry key="Operations" value="false"/>
						<entry key="Workgroup" value="false"/>
					</Map>
				</value>
			</entry>
			<entry key="Approval Types">
				<value>
					<Attributes>
						<Map>
							<entry key="Common Approvals Template">
								<value>
									<Attributes>
										<Map>
											<entry key="defaultApprovalMode" value="serialPoll" />
											<entry key="displayName" value="Owner" />
											<entry key="useDefaultWorkItemConfig" value="true" />
										</Map>
									</Attributes>
								</value>
							</entry>
							<entry key="ManagerOrServiceAccountOwner">
								<value>
									<Attributes>
										<Map>
											<!-- TEMPLATES CAN BE USED TO REDUCE REDUNDANCY, IF KEY NOT HERE, KEY
												FROM TEMPLATE USED -->
											<entry key="template" value="Common Approvals Template" />
											<entry key="displayName" value="Manager or Service Owner" />
											<entry key="getApprovalBusApproversRule" value="method:frameWorkApproversRule" />
											<!-- checkApprovalRequiredRule - This entry contains the name of a custom 
											rule library method, which contains the logic to determine if this approval type
											is needed for a specific request type and approval item. This entry should not be changed
											or removed.
											-->
											<entry key="checkApprovalRequiredRule" value="method:skipOrOverrideorGetSupervisorLevelApprovals" />
										</Map>
									</Attributes>
								</value>
							</entry>		
							<entry key="ApplicationBusApprovers">
								<value>
									<Attributes>
										<Map>
											<!-- TEMPLATES CAN BE USED TO REDUCE REDUNDANCY, IF KEY NOT HERE, KEY
												FROM TEMPLATE USED -->
											<entry key="template" value="Common Approvals Template" />
											<entry key="displayName" value="1st Level Application Business Approvers" />
											<entry key="getApprovalBusApproversRule" value="method:frameWorkApproversRule" />
											<!-- checkApprovalRequiredRule - This entry contains the name of a custom 
											rule library method, which contains the logic to determine if this approval type
											is needed for a specific request type and approval item. This entry should not be changed
											or removed.
											-->
											<entry key="checkApprovalRequiredRule" value="method:skipOrOverrideorGetApplicationFirstLevelApprovals" />
										</Map>
									</Attributes>
								</value>
							</entry>	
							<entry key="AdditionalApplicationBusApprovers">
								<value>
									<Attributes>
										<Map>
											<!-- TEMPLATES CAN BE USED TO REDUCE REDUNDANCY, IF KEY NOT HERE, KEY
												FROM TEMPLATE USED -->
											<entry key="template" value="Common Approvals Template" />
											<entry key="displayName" value="2nd Level Application Business Approvers" />
											<entry key="getApprovalBusApproversRule" value="method:frameWorkApproversRule" />
											<!-- checkApprovalRequiredRule - This entry contains the name of a custom 
											rule library method, which contains the logic to determine if this approval type
											is needed for a specific request type and approval item. This entry should not be changed
											or removed.
											-->
											<entry key="checkApprovalRequiredRule" value="method:skipOrOverrideorGetApplicationSecondLevelApprovals" />
										</Map>
									</Attributes>
								</value>
							</entry>	
							<entry key="EntitlementBusApprovers">
								<value>
									<Attributes>
										<Map>
											<!-- TEMPLATES CAN BE USED TO REDUCE REDUNDANCY, IF KEY NOT HERE, KEY
												FROM TEMPLATE USED -->
											<entry key="template" value="Common Approvals Template" />
											<entry key="displayName" value="1st Level Entitlement Business Approvers" />
											<entry key="getApprovalBusApproversRule" value="method:frameWorkApproversRule" />
											<!-- checkApprovalRequiredRule - This entry contains the name of a custom 
											rule library method, which contains the logic to determine if this approval type
											is needed for a specific request type and approval item. This entry should not be changed
											or removed.
											-->
											<entry key="checkApprovalRequiredRule" value="method:skipOrOverrideorGetEntitlementFirstLevelApprovals" />
										</Map>
									</Attributes>
								</value>
							</entry>	
							<entry key="AdditionalEntitlementBusApprovers">
								<value>
									<Attributes>
										<Map>
											<!-- TEMPLATES CAN BE USED TO REDUCE REDUNDANCY, IF KEY NOT HERE, KEY
												FROM TEMPLATE USED -->
											<entry key="template" value="Common Approvals Template" />
											<entry key="displayName" value="2nd Level Entitlement Business Approvers" />
											<entry key="getApprovalBusApproversRule" value="method:frameWorkApproversRule" />
											<!-- checkApprovalRequiredRule - This entry contains the name of a custom 
											rule library method, which contains the logic to determine if this approval type
											is needed for a specific request type and approval item. This entry should not be changed
											or removed.
											-->
											<entry key="checkApprovalRequiredRule" value="method:skipOrOverrideorGetEntitlementSecondLevelApprovals" />
										</Map>
									</Attributes>
								</value>
							</entry>
							<entry key="RoleBusApprovers">
								<value>
									<Attributes>
										<Map>
											<!-- TEMPLATES CAN BE USED TO REDUCE REDUNDANCY, IF KEY NOT HERE, KEY
												FROM TEMPLATE USED -->
											<entry key="template" value="Common Approvals Template" />
											<entry key="displayName" value="1st Level Role Business Approvers" />
											<entry key="getApprovalBusApproversRule" value="method:frameWorkApproversRule" />
											<!-- checkApprovalRequiredRule - This entry contains the name of a custom 
											rule library method, which contains the logic to determine if this approval type
											is needed for a specific request type and approval item. This entry should not be changed
											or removed.
											-->
											<entry key="checkApprovalRequiredRule" value="method:skipOrOverrideorGetRoleFirstLevelApprovals" />
										</Map>
									</Attributes>
								</value>
							</entry>
								<entry key="AdditionalRoleBusApprovers">
								<value>
									<Attributes>
										<Map>
											<!-- TEMPLATES CAN BE USED TO REDUCE REDUNDANCY, IF KEY NOT HERE, KEY
												FROM TEMPLATE USED -->
											<entry key="template" value="Common Approvals Template" />
											<entry key="displayName" value="2nd Level Role Business Approvers" />
											<entry key="getApprovalBusApproversRule" value="method:frameWorkApproversRule" />
											<!-- checkApprovalRequiredRule - This entry contains the name of a custom 
											rule library method, which contains the logic to determine if this approval type
											is needed for a specific request type and approval item. This entry should not be changed
											or removed.
											-->
											<entry key="checkApprovalRequiredRule" value="method:skipOrOverrideorGetRoleSecondLevelApprovals" />
										</Map>
									</Attributes>
								</value>
							</entry>
							<entry key="Operations">
								<value>
									<Attributes>
										<Map>
											<!-- This Notification Name will override Global Notification Name EmailTemplate-FrameworkApproval-->
											<entry key="notifyEmailTemplate" value="EmailTemplate-FrameworkOperationsReview" />
											<entry key="template" value="Common Approvals Template" />
											<entry key="displayName" value="Operations" />
											<entry key="getApprovalBusApproversRule" value="method:frameWorkApproversRule" />
										</Map>
									</Attributes>
								</value>
							</entry>
							<entry key="Workgroup">
								<value>
									<Attributes>
										<Map>
											<entry key="template" value="Common Approvals Template" />
											<entry key="displayName" value="Workgroup" />
											<entry key="getApprovalBusApproversRule" value="rule:Rule-FrameWorkApproversRule" />
										</Map>
									</Attributes>
								</value>
							</entry>	
							<entry key="BatchOwners">
								<value>
									<Attributes>
										<Map>
											<!-- This Notification Name will override Global Notification Name EmailTemplate-FrameworkApproval-->
											<entry key="notifyEmailTemplate" value="EmailTemplate-FrameworkBatch" />
											<entry key="template" value="Common Approvals Template" />
											<entry key="displayName" value="Batch Request" />
											<entry key="getApprovalBusApproversRule" value="method:frameWorkApproversRule" />
											<!-- checkApprovalRequiredRule - This entry contains the name of a custom 
											rule library method, which contains the logic to determine if this approval type
											is needed for a specific request type and approval item. -->
											<entry key="checkApprovalRequiredRule" value="method:skiporGetBatchLevelApprovals" />
										</Map>
									</Attributes>
								</value>
							</entry>	
						</Map>
					</Attributes>
				</value>			
			</entry>
			<!--  Default WorkItem Config - Define Default values for reminders and escalations -->
			<entry key="Default WorkItem Config">
				<value>
					<Attributes>
						<Map>
							<entry key="disableApprovalNotifications" value="false" />
							<entry key="notifyEmailTemplate" value="EmailTemplate-FrameworkApproval" />
							<entry key="reminderEmailTemplate" value="EmailTemplate-FrameworkReminder" />
							<entry key="escalationEmailTemplate" value="EmailTemplate-FrameworkEscalation" />
							<entry key="accountSelectionNotifyEmailTemplate" value="EmailTemplate-FrameworkAccountSelectionNotification"/>
							<entry key="escalationStyle" value="both" />
							<entry key="hoursBetweenReminders" value="24" />
							<entry key="hoursTillEscalation" value="24" />
							<entry key="hoursTillExpiration" value="168" />
							<entry key="maxReminders" value="3" />
							<entry key="escalationRule" value="Rule-FrameworkEscalationType" />
							<!-- Escalation Types - Dictates how escalations are handled
							Possible values:
							1) sameLevel: execute rule defined in escalationSameLevelRule entry
							2) directManager: escalate to work item owner's direct manager 
							3) emailToManager: does not escalate item, but sends an email to owner's manager and cc's the owner
							-->
							<entry key="escalationType" value="emailToManager" />
							<!--<entry key="electronicSignature" value="SmartServices Electronic Signature" /> -->
							<entry key="electronicSignature" value="No Electronic Signature Required" /> 
							<entry key="escalationSameLevelRule" value="" />
						</Map>
					</Attributes>
				</value>
			</entry>
		</Map>
	</Attributes>
</Custom>
