<sailpoint>
<ImportAction name='merge'>
<ObjectConfig name="Application">
	<!-- LEAVER LOA -->
	<ObjectAttribute categoryName="Leaver" defaultValue="" displayName="Leave Of Absence Options" editMode="Permanent" namedColumn="true" name="loaProcess" required="true">
		<AllowedValues>
			<String>Do Nothing</String>
			<String>Disable Only</String>
			<String>Disable and Remove Entitlements</String>
			<String>Remove Entitlements</String>
			<String>Delete Immediately</String>
			<String>Lock Only</String>
			<String>Extended Rule</String>
		</AllowedValues>
    <Description>Leave of Absence Options</Description>
	</ObjectAttribute>
	<ObjectAttribute categoryName="Leaver"
		defaultValue=""  displayName="Leave Of Absence Extended Rule" editMode="Permanent" name="loaExtendedRule">
		<Description>This rule will be executed in case selected Leave of Absence Options is Extended Rule</Description>
	</ObjectAttribute>
	</ObjectConfig>
	</ImportAction>
</sailpoint>
