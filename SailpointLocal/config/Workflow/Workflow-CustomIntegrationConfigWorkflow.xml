<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Workflow PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Workflow explicitTransitions="true" name="Custom Integration Config workflow">
          
    
    <Variable input="true" name="plan"/>
          
    
    <Variable input="true" name="exampleArgument"/>
          
    
    <Variable input="true" name="identityName"/>
          
    
    <Variable initializer="true" name="trace"/>
          
    
    <Step icon="Start" name="Start" posX="20" posY="20">
                    
        
        <Transition to="GenerateWorkItem"/>
                  
    
    </Step>
          
    
    <Step action="rule:Rule-Generate WorkItem" icon="Default" name="GenerateWorkItem" posX="144" posY="5">
                    
        
        <Transition to="Stop"/>
                  
    
    </Step>
          
    
    <Step icon="Stop" name="Stop" posX="297"/>
        

</Workflow>
