<!--
  Bean definitions for the unit tests.
  Same as the full web app (iiqBeans.xml) except that we use
  a different database with connection parameters defined in 
  cleantest.properties rather than iiq.properties.
-->
<beans default-autowire="byName" default-lazy-init="false" >

    <import resource="iiqBeans.xml"/>

    <bean id="propertyOverrider" class="sailpoint.server.SpringPropertyOverrider">
      <property name="location" value="cleantest.properties"/>
    </bean>

</beans>
