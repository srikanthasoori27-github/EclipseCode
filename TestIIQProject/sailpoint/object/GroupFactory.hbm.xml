<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" [
  <!ENTITY SailPointObject SYSTEM "classpath://sailpoint/object/SailPointObject.hbm.xml">
]>

<!-- (c) Copyright 2008 SailPoint Technologies, Inc., All Rights Reserved. -->

<hibernate-mapping>
  <class name="sailpoint.object.GroupFactory">

    &SailPointObject;

    <property name="name"/>
    <property name="description" type="string" length="1024"/>
    <property name="factoryAttribute"/>
    <property name="enabled" type="boolean"/>
    <property name="lastRefresh" type="sailpoint.persistence.DateType"/>
    <many-to-one embed-xml="false" name="groupOwnerRule" class="sailpoint.object.Rule"/>

  </class>
</hibernate-mapping>
