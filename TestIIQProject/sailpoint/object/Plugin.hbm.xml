<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- (c) Copyright 2016 SailPoint Technologies, Inc., All Rights Reserved. -->

<hibernate-mapping>
  <class name="sailpoint.object.Plugin">

    <id name="id" length="32">
        <generator class="sailpoint.persistence.IdGenerator">
          <param name="uuid_gen_strategy_class">org.hibernate.id.uuid.CustomVersionOneStrategy</param>
        </generator>
    </id>

    <property name="name" type="string" length="255" index="spt_plugin_name_ci"/>
    <property name="created" type="sailpoint.persistence.DateType"/>
    <property name="modified" type="sailpoint.persistence.DateType"/>

    <property name="installDate" type="sailpoint.persistence.DateType"/>
    <property name="displayName" type="string" index="spt_plugin_dn_ci"/>
    <property name="version" type="string"/>
    <property name="disabled" type="boolean"/>
    <property name="rightRequired" type="string"/>
    <property name="minSystemVersion" type="string"/>
    <property name="maxSystemVersion" type="string"/>
    <property name="attributes" type="sailpoint.persistence.XmlType" length="100000000"/>
    <property name="position" type="int"/>

    <property name="certificationLevel">
      <type name="sailpoint.persistence.EnumType">
        <param name="enumClassName">sailpoint.object.Plugin$CertificationLevel</param>
      </type>
    </property>

    <many-to-one embed-xml="false" name="file" class="sailpoint.object.PersistedFile" column="file_id" unique="true" />

  </class>
</hibernate-mapping>
