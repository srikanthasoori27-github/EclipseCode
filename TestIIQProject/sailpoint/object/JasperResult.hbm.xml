<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" [
  <!ENTITY SailPointObject SYSTEM "classpath://sailpoint/object/SailPointObject.hbm.xml">
]>

<!-- (c) Copyright 2008 SailPoint Technologies, Inc., All Rights Reserved. -->

<hibernate-mapping>
  <class name="sailpoint.object.JasperResult">

    &SailPointObject;
    <property name="handlerId" type="string" length="128"/>
    <property name="printXml" type="materialized_clob" length="1073741823"/>
    <property name="pageCount" type="int"/>
    <property name="pagesPerBucket" type="int"/>
    <property name="handlerPageCount" type="int"/>



    <list name="files" table="JasperFiles">
      <key column="result"/>
      <list-index column="idx"/>
      <many-to-many class="sailpoint.object.PersistedFile"/>
    </list>

    <property name="attributes" type="sailpoint.persistence.XmlType" length="100000000"/>

  </class>
</hibernate-mapping>
